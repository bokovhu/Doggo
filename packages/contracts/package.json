{
    "name": "@doggo/contracts",
    "description": "The contracts package",
    "version": "0.0.1",
    "main": "src/index.ts",
    "private": true,
    "dependencies": {
        "react": "*",
        "react-dom": "*",
        "three": "*",
        "rxjs": "*",
        "brain.js": "*",
        "@polkadot/api": "*",
        "@polkadot/api-contract": "*",
        "@polkadot/extension-dapp": "*",
        "commander": "*",
        "marked": "*",
        "marked-terminal": "*"
    },
    "devDependencies": {
        "@types/three": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/marked-terminal": "*",
        "typescript": "*",
        "esbuild": "*",
        "del-cli": "*"
    },
    "scripts": {
        "build": "cargo contract build --target wasm --release --manifest-path src/doggo/Cargo.toml",
        "watch": "cargo watch -w src/ -x \"contract build --target wasm --release --manifest-path src/doggo/Cargo.toml\"",
        "start": "echo \"No start script provided\"",
        "serve": "substrate-contracts-node --dev --base-path ./.node-data",
        "clean": "del compiled types dist target build",
        "dev": "cargo watch -w src/ -x \"contract build --target wasm --release --manifest-path src/doggo/Cargo.toml\""
    }
}